        <servlet-name>DownloadServlet</servlet-name>
        <url-pattern>/DownloadServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ClicksServlet</servlet-name>
        <servlet-class>com.edu.servlet.ClicksServlet</servlet-class>
    </servlet>
    <servlet-mapping>
